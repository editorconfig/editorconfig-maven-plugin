root = true

["*.java"]
indent_size = 4
indent_style = tab
tab_width = 2
insert_final_newline = false
charset = utf-8
trim_trailing_whitespace = false
end_of_line = crlf

["*.kt"]
indent_size = 2
indent_style = space
tab_width = 2
insert_final_newline = false
charset = utf-8
trim_trailing_whitespace = false
end_of_line = crlf
